{
    "manifest_version": 2,
    "name": "GOP filter",
    "version": "1.2",
    "description": "Lets give them better names to brighten your day.",
    "icons": { "16": "images/16.png",
        "48": "images/48.png",
        "128": "images/128.png" },
    "content_scripts":
    [
     {
     "matches": ["*://*/*"],
     "js": ["content_script.js"],
     "run_at": "document_end"
     }
     ]
}